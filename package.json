{
  "name": "github-license-summary",
  "license": "MIT",
  "description": "An extension to show license summary on Github repository",
  "scripts": {
    "js:lint": "yarn run eslint src",
    "js:lint:watch": "yarn run esw --watch src",
    "webext:build": "yarn run web-ext build",
    "webext:lint": "yarn run web-ext lint",
    "webext:run": "yarn run web-ext run",
    "build": "npm-run-all js:lint webext:build",
    "lint": "npm-run-all js:lint webext:lint",
    "run": "npm-run-all --parallel js:lint:watch webext:run"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^3.1.2",
    "npm-run-all": "^4.1.1",
    "web-ext": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.16.2"
  }
}
